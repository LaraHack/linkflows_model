@prefix : <http://purl.org/nanopub/temp/linkflows/sample-paper/camera-ready/s6#> .
@prefix p1nanopub: <http://purl.org/nanopub/temp/linkflows/sample-paper/camera-ready/s6_p1#> .
@prefix p2nanopub: <http://purl.org/nanopub/temp/linkflows/sample-paper/camera-ready/s6_p2#> .
@prefix p3nanopub: <http://purl.org/nanopub/temp/linkflows/sample-paper/camera-ready/s6_p3#> .
@prefix p4nanopub: <http://purl.org/nanopub/temp/linkflows/sample-paper/camera-ready/s6_p4#> .
@prefix p5nanopub: <http://purl.org/nanopub/temp/linkflows/sample-paper/camera-ready/s6_p5#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix pav: <http://purl.org/pav/> .
@prefix np: <http://www.nanopub.org/nschema#> .
@prefix npx: <http://purl.org/nanopub/x/> .
@prefix po: <http://www.essepuntato.it/2008/12/pattern#> .
@prefix co: <http://purl.org/co/> .
@prefix deo: <http://purl.org/spar/deo/> .
@prefix doco: <http://purl.org/spar/doco/> .
@prefix c4o: <http://purl.org/spar/c4o/> .

:Head {
	: np:hasAssertion :assertion ;
		np:hasProvenance :provenance ;
		np:hasPublicationInfo :pubinfo ;
		a np:Nanopublication .
}

:assertion {
	:section-conclusion a doco:Section, deo:Discussion, deo:FutureWork ;
		po:containsAsHeader :section-future-work-title ;
		po:contains
			p1nanopub:paragraph-1 ,
			p2nanopub:paragraph-2 ,
			p3nanopub:paragraph-3 ,
			p4nanopub:paragraph-4 ,
			p5nanopub:paragraph-5 ;
		co:firstItem [
		co:itemContent :section-future-work-title ;
		co:nextItem [
		      co:itemContent p1nanopub:paragraph-1 ;
		      co:nextItem [
			co:itemContent p2nanopub:paragraph-2 ;
			co:nextItem [
			  co:itemContent p3nanopub:paragraph-3 ;
			  co:nextItem [
			    co:itemContent p4nanopub:paragraph-4 ;
			    co:nextItem [
			      co:itemContent p5nanopub:paragraph-5 ] ] ] ] ] ] .
	:section-future-work-title a doco:SectionTitle ;
		c4o:hasContent "Limitations and future directions" .

	p1nanopub: npx:introduces p1nanopub:paragraph-1 .
	p2nanopub: npx:introduces p2nanopub:paragraph-2 .
	p3nanopub: npx:introduces p3nanopub:paragraph-3 .
	p4nanopub: npx:introduces p4nanopub:paragraph-4 .
	p5nanopub: npx:introduces p5nanopub:paragraph-5 .
}

:provenance {
	:assertion prov:wasAttributedTo <https://orcid.org/0000-0003-0530-4305> ;
		 prov:hadPrimarySource <http://dx.doi.org/10.3233/DS-170012> .
}

:pubinfo {
	: dc:created "2018-09-17T18:05:11+01:00"^^xsd:dateTime ;
		pav:createdBy  <https://orcid.org/0000-0002-7114-6459> ;
		a npx:ExampleNanopub .
}

