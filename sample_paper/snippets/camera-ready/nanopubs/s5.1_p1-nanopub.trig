:Head {
	: np:hasAssertion :assertion ;
		np:hasProvenance :provenance ;
		np:hasPublicationInfo :pubinfo ;
		a np:Nanopublication .
}

:assertion {
	:paragraph-1 a doco:Paragraph ;
		c4o:hasContent :snippet-paragraph-1 .
	:snippet-paragraph-1
		snp:snippetURI """Understanding how scholarly documents can be segmented into structural components, which can then be manipulated independently for different purposes, is a topic that my colleagues and I have studied extensively in the past. The main outcome of this research [9] is the proposal of a theory of structural patterns for digital documents that are sufficient to express what most users need in terms of document constituents and components when writing scholarly papers.""" .
}

:provenance {
	:assertion prov:wasAttributedTo <https://orcid.org/0000-0003-0530-4305> .
}

:pubinfo {
	: dc:created "2018-09-10T18:05:11+01:00"^^xsd:dateTime ;
		pav:createdBy ex:linkflowsAuthoringTool ;
		a npx:ExampleNanopub .
}

