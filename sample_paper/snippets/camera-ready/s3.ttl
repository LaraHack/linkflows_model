@prefix doco: <http://purl.org/spar/doco/> .
@prefix deo: <http://purl.org/spar/deo/> .
@prefix po: <http://www.essepuntato.it/2008/12/pattern#> .
@prefix co: <http://purl.org/co/> .
@prefix c4o: <http://purl.org/spar/c4o> .

# change this to proper URLs
@prefix snp: <http://linkflows.com/snippets/> .
@prefix : <http://linkflows.com/snippets/sample_paper/camera-ready/> .

:section-introduction a doco:Section , deo:Motivation ;
  po:containsAsHeader :section-motivation-title ;
  po:contains
    :paragraph-1 ,
    :paragraph-2 ,
    :paragraph-3 ,
    :paragraph-4 ,
    :paragraph-5 ,
    :paragraph-6 ,
    :paragraph-7 ;
  co:firstItem [
    co:itemContent :section-motivation-title ;
    co:nextItem [
      co:itemContent :paragraph-1 ;
      co:nextItem [
        co:itemContent :paragraph-2 ;
        co:nextItem [
          co:itemContent :paragraph-3 ;
          co:nextItem [
            co:itemContent :paragraph-4 ;
            co:nextItem [
              co:itemContent :paragraph-5 ;
              co:nextItem [
                co:itemContent :paragraph-6 ;
                co:nextItem [
                  co:itemContent :paragraph-7 ] ] ] ] ] ] ] ] .

:section-motivation-title a doco:SectionTitle ;
    c4o:hasContent "A pathway from syntax to semantics" .

:paragraph-1 a doco:Paragraph ;
  snp:snippetURI <http://../sample_paper/snippets/camera-ready/s3_p1.ttl> .

:paragraph-2 a doco:Paragraph ;
  snp:snippetURI <http://../sample_paper/snippets/camera-ready/s3_p2.ttl> .

:paragraph-3 a doco:Paragraph ;
  snp:snippetURI <http://../sample_paper/snippets/camera-ready/s3_p3.ttl> .

:paragraph-4 a doco:Paragraph ;
  snp:snippetURI <http://../sample_paper/snippets/camera-ready/s3_p4.ttl> .

:paragraph-5 a doco:Paragraph ;
  snp:snippetURI <http://../sample_paper/snippets/camera-ready/s3_p5.ttl> .

:paragraph-6 a doco:Paragraph ;
  snp:snippetURI <http://../sample_paper/snippets/camera-ready/s3_p6.ttl> .

:paragraph-7 a doco:Paragraph ;
  snp:snippetURI <http://../sample_paper/snippets/camera-ready/s3_p7.ttl> .
