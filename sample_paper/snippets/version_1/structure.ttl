@prefix fabio: <http://purl.org/spar/fabio/> .
@prefix po: <http://www.essepuntato.it/2008/12/pattern#> .
@prefix co: <http://purl.org/co/> .
@prefix doco: <http://purl.org/spar/doco/> .
@prefix deo: <http://purl.org/spar/deo/> .

# change this to proper URLs
@prefix snp: <http://linkflows.com/snippets/> .
@prefix : <http://linkflows.com/sample_paper/> .

:sample_paper a fabio:JournalArticle ;
  po:contains
    :section-abstract ,
    :section-introduction ,
    :section-related-works ,
    :section-motivation ,
    :section-contribution-future-directions ,
    :section-conclusions ;
  co:firstItem [
    co:itemContent :section-abstract ;
    co:nextItem [
      co:itemContent :section-introduction ;
      co:nextItem [
        co:itemContent :section-related-works ;
        co:nextItem [
          co:itemContent :section-motivation ;
          co:nextItem [
            co:itemContent :section-contribution-future-directions ;
            co:nextItem [
              co:itemContent :section-conclusions ] ] ] ] ] ] .

:section-abstract a doco:Abstract ;
  snp:snippetURI <http://../sample_paper/snippets/version_1/abstract.ttl> .

:section-introduction a doco:Section, deo:Introduction ;
  snp:snippetURI <http://../sample_paper/snippets/version_1/s1.ttl> .

:section-related-works a doco:Section, deo:RelatedWork ;
  snp:snippetURI <http://../sample_paper/snippets/version_1/s2.ttl> .

:section-motivation a doco:Section, deo:Motivation ;
  snp:snippetURI <http://../sample_paper/snippets/version_1/s3.ttl> .

:section-contribution-future-directions a doco:Section, deo:Contribution ;
  snp:snippetURI <http://../sample_paper/snippets/version_1/s4.ttl> .

:section-conclusions a doco:Section, deo:Conclusion ;
  snp:snippetURI <http://../sample_paper/snippets/version_1/s5.ttl> .
