@prefix doco: <http://purl.org/spar/doco/> .
@prefix deo: <http://purl.org/spar/deo/> .
@prefix po: <http://www.essepuntato.it/2008/12/pattern#> .
@prefix co: <http://purl.org/co/> .
@prefix c4o: <http://purl.org/spar/c4o> .

# change this to proper URLs
@prefix snp: <http://linkflows.com/snippets/> .
@prefix : <http://linkflows.com/snippets/sample_paper/version_1/> .

:section-introduction a doco:Section , deo:Introduction ;
  po:containsAsHeader :section-introduction-title ;
  po:contains
    :paragraph-1 ,
    :paragraph-2 ,
    :paragraph-3 ,
    :paragraph-4 ,
    :paragraph-5 ,
    :paragraph-6 ,
    :paragraph-7 ,
    :paragraph-8 ;
  co:firstItem [
    co:itemContent :section-introduction-title ;
    co:nextItem [
      co:itemContent :paragraph-1 ;
      co:nextItem [
        co:itemContent :paragraph-2 ;
        co:nextItem [
          co:itemContent :paragraph-3 ;
          co:nextItem [
            co:itemContent :paragraph-4 ;
            co:nextItem [
              co:itemContent :paragraph-5 ;
              co:nextItem [
                co:itemContent :paragraph-6 ;
                co:nextItem [
                  co:itemContent :paragraph-7 ;
                  co:nextItem [
                    co:itemContent :paragraph-8 ] ] ] ] ] ] ] ] ] .

:section-introduction-title a doco:SectionTitle ;
    c4o:hasContent "Print, Digital, and Semantic Publishing" .

:paragraph-1 a doco:Paragraph ;
  snp:snippetURI <http://../sample_paper/snippets/version_1/s1_p1.ttl> .

:paragraph-2 a doco:Paragraph ;
  snp:snippetURI <http://../sample_paper/snippets/version_1/s1_p2.ttl> .

:paragraph-3 a doco:Paragraph ;
  snp:snippetURI <http://../sample_paper/snippets/version_1/s1_p3.ttl> .

:paragraph-4 a doco:Paragraph ;
  snp:snippetURI <http://../sample_paper/snippets/version_1/s1_p4.ttl> .

:paragraph-5 a doco:Paragraph ;
  snp:snippetURI <http://../sample_paper/snippets/version_1/s1_p5.ttl> .

:paragraph-6 a doco:Paragraph ;
  snp:snippetURI <http://../sample_paper/snippets/version_1/s1_p6.ttl> .

:paragraph-7 a doco:Paragraph ;
  snp:snippetURI <http://../sample_paper/snippets/version_1/s1_p7.ttl> .

:paragraph-8 a doco:Paragraph ;
  snp:snippetURI <http://../sample_paper/snippets/version_1/s1_p8.ttl> .
