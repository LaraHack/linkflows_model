@prefix doco: <http://purl.org/spar/doco/> .
@prefix deo: <http://purl.org/spar/deo/> .
@prefix po: <http://www.essepuntato.it/2008/12/pattern#> .
@prefix co: <http://purl.org/co/> .
@prefix c4o: <http://purl.org/spar/c4o> .

# change this to proper URLs
@prefix snp: <http://linkflows.com/snippets/> .
@prefix : <http://linkflows.com/snippets/sample_paper/version_1/> .

:section-introduction a doco:Section , deo:Conclusion ;
  po:containsAsHeader :section-conclusion-title ;
  po:contains
    :paragraph-1 ,
    :paragraph-2 ;
  co:firstItem [
    co:itemContent :section-conclusion-title ;
    co:nextItem [
      co:itemContent :paragraph-1 ;
      co:nextItem [
        co:itemContent :paragraph-2 ] ] ] .

:section-conclusion-title a doco:SectionTitle ;
    c4o:hasContent "Conclusions" .

:paragraph-1 a doco:Paragraph ;
  snp:snippetURI <http://../sample_paper/snippets/version_1/s7_p1.ttl> .

:paragraph-2 a doco:Paragraph ;
  snp:snippetURI <http://../sample_paper/snippets/version_1/s7_p2.ttl> .
