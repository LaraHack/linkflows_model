@prefix doco: <http://purl.org/spar/doco/> .
@prefix deo: <http://purl.org/spar/deo/> .
@prefix po: <http://www.essepuntato.it/2008/12/pattern#> .
@prefix co: <http://purl.org/co/> .
@prefix c4o: <http://purl.org/spar/c4o> .

# change this to proper URLs
@prefix snp: <http://linkflows.com/snippets/> .
@prefix : <http://linkflows.com/snippets/sample_paper/version_1/> .

:section-introduction a doco:Section , deo:Abstract ;
  po:containsAsHeader :section-abstract-title ;
  po:contains
    :paragraph-1 ;
  co:firstItem [
    co:itemContent :section-abstract-title ;
    co:nextItem [
      co:itemContent :paragraph-1 ] ] .

:section-abstract-title a doco:SectionTitle ;
    c4o:hasContent "Abstract" .

:paragraph-1 a doco:Paragraph ;
  snp:snippetURI <http://../sample_paper/snippets/version_1/abstract.txt> .
